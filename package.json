{
  "name": "solidown",
  "version": "1.2.0",
  "description": "A high performance markdown component for SolidJS",
  "keywords": [
    "markdown",
    "remarkable",
    "solid",
    "solidjs",
    "solid-markdown",
    "commonmark",
    "component"
  ],
  "homepage": "https://yellowsink.vercel.app/solidown",
  "author": "Yellowsink <yellowsink@protonmail.com> (https://yellowsink.vercel.app/)",
  "repository": "github:yellowsink/solidown",
  "scripts": {
    "build": "rollup -c"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/remarkable": "^2.0.3",
    "babel-preset-solid": "^1.2.6",
    "rollup": "^2.62.0",
    "rollup-preset-solid": "^1.0.1",
    "solid-js": "^1.2.6",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "solid-js": "^1.2.0"
  },
  "dependencies": {
    "remarkable": "^2.0.1"
  },
  "source": "lib/index.tsx",
  "main": "dist/cjs/index.js",
